{
    "manifest_version": 3,
    "name": "Readibility Checker",
    "version": "1.0",
    "description": "Extracts text from a webpage and assigns readibility score",
    "permissions": ["scripting"],
    "action": {
      "default_icon": "icon.png",
      "default_title": "Readibility Checker",
      "default_popup": "popup.html"
    },
    "host_permissions": ["<all_urls>"]
  }
  